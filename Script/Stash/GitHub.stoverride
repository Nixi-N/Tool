http:

  mitm:
    - "raw.githubusercontent.com"

  script:
    - match: ^https:\/\/raw\.githubusercontent\.com\/
      name: "RawBlob_4"
      type: request
      require-body: false
      timeout: 20

script-providers:
  "RawBlob_4":
    url: br.js
    interval: 86400